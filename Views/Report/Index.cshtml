
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>举报类型管理列表</title>
    <link rel="shortcut icon" href="favicon.ico">
    <link href="/css/bootstrap.min14ed.css?v=3.3.6" rel="stylesheet">
    <link href="/css/font-awesome.min93e3.css?v=4.4.0" rel="stylesheet">
    <!-- jqgrid-->
    <link href="/css/plugins/jqgrid/ui.jqgridffe4.css?0820" rel="stylesheet">
    <link href="/css/animate.min.css" rel="stylesheet">
    <link href="/css/style.min862f.css?v=4.1.0" rel="stylesheet">
    <script type="text/javascript" src="/Scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="/Scripts/amazeui.js"></script>
    <link type="text/css" rel="stylesheet" href="/Content/amazeui.min.css" />
    <script src="/Scripts/kkpager-master/src/kkpager.js"></script>
    <link type="text/css" href="/Scripts/kkpager_blue.css" rel="stylesheet" />
    <script src="~/Scripts/Report/ReportList.js"></script>
    <script type="text/javascript">
        jQuery(function () {
            ReportList.search();
        });
        function AddReport() {
            window.location.href = "/AlterReport";
        }
    </script>
</head>
<body class="gray-bg">
    <input type="hidden" id="selectedid" />
    <div class="wrapper wrapper-content  animated fadeInRight">
        <div class="row">
            <div class="col-sm-12">
                <div class="ibox ">
                    <div class="ibox-title">
                        <h5>举报类型管理列表</h5>
                    </div>
                    <div class="ibox-content">
                        <div class="am-g">
                            <div class="am-u-sm-12 am-u-md-12">
                                <div class="am-fl" style="margin-right:10px;"><button type="button" class="am-btn am-btn-primary  am-u-lg-centered" onclick="AddReport()">新增</button></div>
                            </div>
                        </div>
                        <hr>
                        <div class="jqGrid_wrapper">
                            <div class="am-g">
                                <div class="am-u-sm-12" style="min-height:200px;">
                                    <table class="am-table am-table-striped am-table-hover table-main">
                                        <thead>
                                            <tr>
                                                <th class="table-title">举报类型</th>
                                                <th class="table-title">备注</th>
                                                <th class="table-author am-hide-sm-only">操作</th>
                                            </tr>
                                        </thead>
                                        <tbody id="tables"></tbody>
                                    </table>
                                    <div id="kkpager"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--删除开始-->
    <div class="am-modal am-modal-prompt" tabindex="-1" id="delteReport">
        <div class="am-modal-dialog">
            <div class="am-modal-hd">删除举报类型</div>
            <div class="am-modal-bd">
                你，确定要删除这条记录吗？
            </div>
            <div class="am-modal-footer">
                <span class="am-modal-btn" data-am-modal-cancel>取消</span>
                <span class="am-modal-btn" data-am-modal-confirm>确定</span>
            </div>
        </div>
    </div>
    <!--删除结束-->
</body>
<!-- Mirrored from www.zi-han.net/theme/hplus/table_jqgrid.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 20 Jan 2016 14:20:02 GMT -->
</html>